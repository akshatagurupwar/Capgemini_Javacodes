package com.capgi.day5;

public class PalindromeString {
	public static void main(String args[])
	{
		String str="Keep";
		boolean flag=true;
        int i = 0, j = str.length() - 1;
        
        
        while (i < j) {
        	 
            if (str.charAt(i) != str.charAt(j))
            {
                flag=false;
            }
 
            i++;
            j--;
        }
 
        if(flag)
        {
        	System.out.println("Palindrome");
        }
        else
        {
        	System.out.println(" Not Palindrome");
        }

        	
        
    }
     
	}


